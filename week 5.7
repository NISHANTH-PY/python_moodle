def get_second_word_in_uppercase(sentence):
    # Split the sentence into words
    words = sentence.split()

    # Check if there are at least 2 words
    if len(words) < 2:
        return "LESS"

    # Get the second word and convert it to uppercase
    second_word = words[1].upper()
    return second_word

# Read input sentence
input_sentence = input()

# Get the result and print it
result = get_second_word_in_uppercase(input_sentence)
print(result)

